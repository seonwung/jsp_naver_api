<div id="iSldFnctSetup18" class="hidden">
	imageSlideSetupSeq=18,cnvrsVe=1,stopTime=5,pcCo=2,cnvrsMth=SLIDE,pcMgWidth=100,isImageNoHandlr=true,isCnvrsHandlr=true,isPlayStopHandlr=true
</div>

<script type="text/javascript">
$(document).ready(function() {
	// 2023.09.19. 상단에서 처리
	// full popup 갯수 표시 //
	var fullPopupLength = $('#slider18').find('li:not(.slick-cloned)').length;
	$('a.openFullPopup span').text(fullPopupLength);

	$("#slider18").slick({
		slidesToShow: 2,
		slidesToScroll: 1,
		dots: true,
		arrows: true,
		autoplay: true,
		autoplaySpeed: 5000,
		speed: 1000,
		infinite: true,
		pauseOnHover: false,
		rows: 2,
		//slidesPerRow: 2,
		responsive: [
			{
				breakpoint: 768,
				settings: {
					rows: 2,
					slidesPerRow: 1,
					slidesToShow: 1,
					slidesToScroll: 1
				}
			}

		],
		fade: false,
		cssEase: 'cubic-bezier(0.7, 0, 0.3, 1)',
		appendDots: $('#wrap-slider18 .control .paging'),
		prevArrow: '#wrap-slider18 .prevnext .prev',
		nextArrow: '#wrap-slider18 .prevnext .next'
	});

	$('#wrap-slider18 button.play').on('click', function () {
		$("#slider18").slick("slickPlay");
		$(this).removeClass('on');
		$('#wrap-slider18 button.stop').addClass('on');
	});
	$('#wrap-slider18 button.stop').on('click', function () {
		$("#slider18").slick("slickPause");
		$(this).removeClass('on');
		$('#wrap-slider18 button.play').addClass('on');
	});

	$('.openFullPopup').on('click', function(e) {
		var aLength = $("#aLength").val();
		if(aLength > 0){
			$('.main_full_popup').show()
		}
		
		var isManagerChk = $("#isManagerChk").val();
		if(aLength == 0 && isManagerChk == "true"){
			$(this).attr('class', 'openFullPopup _blockUI widgetManagerUrl new_win');
			$(this).attr('target', '_blank');
			$(this).attr('href', '/imageSlide/fnctMngr/kr/18/setupUpdtView.do');
			console.log("managerChk true");
		}
	});

	// full popup 갯수 표시 //
	/*var fullPopupLength = $('#slider18').find('li:not(.slick-cloned)').length;
	$('a.openFullPopup span').text(fullPopupLength); */

	var readyFullPopup = $('.main_full_popup').css('display');	// 초기로딩시 풀팝업 display 값

	// fullpopup 오픈조건 : 1)팝업내용이 1개이상 있어야함, 2)쿠키값이 notsee 가 아니어야 함, 3) css의 main_full_popup div 에 display:none 이 아니어야 함.
	if(parseInt(fullPopupLength) > 0 && getCookie('slider18') != "notsee" && readyFullPopup != "none") {
		$('.main_full_popup').css('visibility', 'visible');
		$('.main_full_popup').show();
		$('.main_full_popup a').filter(':first').focus();
	} else {
		$('.main_full_popup').hide();
	}

	// full popup 닫기 //
	$('.wrap-full-popup .noMore .closeWin').on('click', function(e) {
		e.preventDefault();
		$('#slider18').slick('slickPause');
		$('.main_full_popup').fadeOut();
	});

	// full popup 열고닫기 //
	$('.openFullPopup').on('click', function(e) {
		
		var aLength = $("#aLength").val();
		if(aLength > 0){
			e.preventDefault();
			if($('.main_full_popup').css('dispaly') == "block") {
				$('#slider18').slick('slickPause');
				$('.main_full_popup').fadeOut();
			} else {
				$('.main_full_popup').css('visibility', 'visible').fadeIn();
				$('#slider18').slick('slickSetOption', {}, true);
				$('#slider18').slick('slickPlay');
				$('.main_full_popup a').filter(':first').focus();
			}
		}
		
		var isManagerChk = $("#isManagerChk").val();
		if(aLength == 0 && isManagerChk == "true"){
			$(this).attr('class', 'openFullPopup _blockUI widgetManagerUrl new_win');
			$(this).attr('target', '_blank');
			$(this).attr('href', '/imageSlide/fnctMngr/kr/18/setupUpdtView.do');
			console.log("managerChk true");
		}
	});

	// full popup 오늘하루 열지않음 //
	$('.wrap-full-popup .noMore :checkbox').on('click', function(e) {
		var noSeeNm = e.target.value;
		if($(this).prop('checked')) {
			if(noSeeNm == 'day') {
				setCookie('slider18', 'notsee', 1); 
				$('.main_full_popup').fadeOut();
			} else {
				setCookie('slider18', 'notsee', 7); // 20220729 일주일 추가 
				$('.main_full_popup').fadeOut();
			}
		}
	});

	// 포커스로 스킵메뉴 이동시 풀팝업 닫히게 //
	$('.accessibility01').on('focus', function(		) {
		$('#slider18').slick('slickPause');
		$('.main_full_popup').fadeOut();
	});

});
</script>



	  </div>
	</div>
