<script>
	$(document).ready(function() {

		// 현재페이지번호 / 전체페이지수 //
		$('#slider13').on('init', function(event, slick) {
//			console.log(slick.slideCount);
//			console.log(slick.currentSlide + 1);
			$('#wrap-slider13 .numbering > .total').text(slick.slideCount);
		});
		$('#slider13').on('afterChange', function(event, slick, currentSlide){
			$('#wrap-slider13 .numbering > .now').text(currentSlide+1);
		});

		$("#slider13").slick({
			slidesToShow: 1,
			slidesToScroll: 1,
			dots: true,
			arrows: true,
			autoplay: true,
			autoplaySpeed: 5000,
			speed: 1000,
			infinite: true,
			pauseOnHover: false,
			fade: false,
			cssEase: 'cubic-bezier(0.7, 0, 0.3, 1)',
			appendDots: $('#wrap-slider13 .control .paging'),
			prevArrow: '#wrap-slider13 .prevnext .prev',
			nextArrow: '#wrap-slider13 .prevnext .next'
		});

		$('#wrap-slider13 button.play').on('click', function () {
			$("#slider13").slick("slickPlay");
			$(this).removeClass('on');
			$('#wrap-slider13 button.stop').addClass('on');
		});
		$('#wrap-slider13 button.stop').on('click', function () {
			$("#slider13").slick("slickPause");
			$(this).removeClass('on');
			$('#wrap-slider13 button.play').addClass('on');
		});
	});
</script>


 
  </div> 
