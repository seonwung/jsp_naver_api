<script>
	$(document).ready(function() {

		// 현재페이지번호 / 전체페이지수 //
		$('#slider11').on('init', function(event, slick) {
//			console.log(slick.slideCount);
//			console.log(slick.currentSlide + 1);
			$('#wrap-slider11 .numbering > .total').text(slick.slideCount);
		});
		$('#slider11').on('afterChange', function(event, slick, currentSlide){
			$('#wrap-slider11 .numbering > .now').text(currentSlide+1);
		});

		$("#slider11").slick({
			slidesToShow: 3,
			slidesToScroll: 1,
			dots: true,
			arrows: true,
			autoplay: true,
			autoplaySpeed: 5000,
			speed: 1000,
			infinite: true,
			pauseOnHover: false,
			responsive: [
				{
					breakpoint: 768,
					settings: {
						slidesToShow: 2,
						slidesToScroll: 2,
					}
				},
				{
					breakpoint: 480,
					settings: {
						slidesToShow: 1,
						slidesToScroll: 1
					}
				}
			],

			fade: false,
			cssEase: 'cubic-bezier(0.7, 0, 0.3, 1)',
			appendDots: $('#wrap-slider11 .control .paging'),
			prevArrow: '#wrap-slider11 .prevnext .prev',
			nextArrow: '#wrap-slider11 .prevnext .next'
		});

		$('#wrap-slider11 button.play').on('click', function () {
			$("#slider11").slick("slickPlay");
			$(this).removeClass('on');
			$('#wrap-slider11 button.stop').addClass('on');
		});
		$('#wrap-slider11 button.stop').on('click', function () {
			$("#slider11").slick("slickPause");
			$(this).removeClass('on');
			$('#wrap-slider11 button.play').addClass('on');
		});
	});
</script>


 
